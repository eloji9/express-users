<h2>
    Your Rooms
</h2>
<ul>
    {{#each roomArray}}
        <li>
            <h3>
                {{this.name}}
            </h3>
            <img src="{{this.pictureUrl}}">
        </li>
    {{else}}
        <p>
            You don't have any rooms yet.
            <a href="/room/add">Add a Room</a>
        </p>
    {{/each}}
</ul>